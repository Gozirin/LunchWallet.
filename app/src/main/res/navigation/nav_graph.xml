<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/dashBoardLunchWalletFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.main.MainFragment"
        android:label="main_fragment"
        tools:layout="@layout/main_fragment" />
    <fragment
        android:id="@+id/onboardingScreenFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.main.onboarding.ui.onboarding.OnboardingScreenFragment"
        android:label="fragment_onboarding_screen"
        tools:layout="@layout/fragment_onboarding_screen" >
        <action
            android:id="@+id/action_onboardingScreenFragment_to_landingPageFragment"
            app:destination="@id/landingPageFragment" />
    </fragment>
    <fragment
        android:id="@+id/landingPageFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.main.onboarding.ui.landingpage.LandingPageFragment"
        android:label="fragment_landing_page"
        tools:layout="@layout/fragment_landing_page" >
        <action
            android:id="@+id/action_landingPageFragment_to_selectTypeFragment"
            app:destination="@id/selectTypeFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.main.signup.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" >
        <action
            android:id="@+id/action_signUpFragment_to_checkMailFragment"
            app:destination="@id/checkMailFragment" />
    </fragment>
    <fragment
        android:id="@+id/checkMailFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.main.signup.ui.checkmail.CheckMailFragment"
        android:label="fragment_check_mail"
        tools:layout="@layout/fragment_check_mail" >
        <action
            android:id="@+id/action_checkMailFragment_to_confirmMailFragment"
            app:destination="@id/confirmMailFragment" />
    </fragment>
    <fragment
        android:id="@+id/confirmMailFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.main.signup.ui.confirmMail.ConfirmMailFragment"
        android:label="fragment_confrim_mail_frgament"
        tools:layout="@layout/fragment_confrim_mail_frgament" >
    </fragment>
    <fragment
        android:id="@+id/selectTypeFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.main.signup.ui.selectType.SelectTypeFragment"
        android:label="fragment_select_type"
        tools:layout="@layout/fragment_select_type" >
        <action
            android:id="@+id/action_selectTypeFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/foodBeneficiaryDashboardFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.dashboard.FoodBeneficiaryDashboardFragment"
        android:label="fragment_food_beneficiary_dashboard"
        tools:layout="@layout/fragment_food_beneficiary_dashboard" />
    <fragment
        android:id="@+id/notificationsScreenFragment"
        android:name="com.example.lunchwallet.onboarding.ui.notifications.ui.NotificationsScreenFragment"
        android:label="fragment_notifications_screen"
        tools:layout="@layout/fragment_notifications_screen" />

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_resetPasswordFragment"
            app:destination="@id/resetPasswordFragment" />
        <action
            android:id="@+id/action_loginFragment_to_foodBeneficiaryDashboardFragment"
            app:destination="@id/foodBeneficiaryDashboardFragment" />
        <action
            android:id="@+id/action_loginFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/resetPasswordFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.resetpassword.ResetPasswordFragment"
        android:label="fragment_reset_password"
        tools:layout="@layout/fragment_reset_password" >
        <action
            android:id="@+id/action_resetPasswordFragment_to_checkMailResetPasswordFragment"
            app:destination="@id/checkMailResetPasswordFragment" />
    </fragment>
    <fragment
        android:id="@+id/createNewPasswordFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.resetpassword.CreateNewPasswordFragment"
        android:label="fragment_create_new_password"
        tools:layout="@layout/fragment_create_new_password" />
    <fragment
        android:id="@+id/checkMailResetPasswordFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.resetpassword.CheckMailResetPasswordFragment"
        android:label="fragment_check_mail_reset_password"
        tools:layout="@layout/fragment_check_mail_reset_password" >
        <action
            android:id="@+id/action_checkMailResetPasswordFragment_to_createNewPasswordFragment"
            app:destination="@id/createNewPasswordFragment" />
        <action
            android:id="@+id/action_checkMailResetPasswordFragment_to_resetPasswordFragment"
            app:destination="@id/resetPasswordFragment" />
    </fragment>
    <fragment
        android:id="@+id/optionMealFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.main.signup.uploadmeals.OptionMealFragment"
        android:label="fragment_option_meal"
        tools:layout="@layout/fragment_option_meal" >
        <action
            android:id="@+id/action_optionMealFragment_to_messageAlertFragment"
            app:destination="@id/messageAlertFragment" />
    </fragment>
    <fragment
        android:id="@+id/messageAlertFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.main.signup.uploadmeals.MessageAlertFragment"
        android:label="fragment_message_alert"
        tools:layout="@layout/fragment_message_alert" >
        <action
            android:id="@+id/action_messageAlertFragment_to_accountLunchWalletFragment2"
            app:destination="@id/accountLunchWalletFragment2" />
    </fragment>
    <fragment
        android:id="@+id/uploadMealFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.main.signup.uploadmeals.UploadMealFragment"
        android:label="UploadMealFragment" >
        <action
            android:id="@+id/action_uploadMealFragment_to_optionMealFragment"
            app:destination="@id/optionMealFragment" />
    </fragment>
    <fragment
        android:id="@+id/dashBoardLunchWalletFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.dashboard.DashBoardLunchWalletFragment"
        android:label="fragment_dash_board_lunch_wallet"
        tools:layout="@layout/fragment_dash_board_lunch_wallet" >
        <action
            android:id="@+id/action_dashBoardLunchWalletFragment_to_uploadMealFragment"
            app:destination="@id/uploadMealFragment" />
    </fragment>
    <fragment
        android:id="@+id/accountLunchWalletFragment2"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.main.signup.uploadmeals.AccountLunchWalletFragment"
        android:label="fragment_account_lunch_wallet"
        tools:layout="@layout/fragment_account_lunch_wallet" />
</navigation>