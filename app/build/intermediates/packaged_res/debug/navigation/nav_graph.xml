<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/kitchenStaffDashBoardFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.main.MainFragment"
        android:label="main_fragment"
        tools:layout="@layout/main_fragment" />
    <fragment
        android:id="@+id/onboardingScreenFragment"
        android:name="com.example.lunchwallet.onboarding.ui.onboarding.OnboardingScreenFragment"
        android:label="fragment_onboarding_screen"
        tools:layout="@layout/fragment_onboarding_screen" />
    <fragment
        android:id="@+id/landingPageFragment"
        android:name="com.example.lunchwallet.onboarding.ui.landingpage.LandingPageFragment"
        android:label="fragment_landing_page"
        tools:layout="@layout/fragment_landing_page" />
    <fragment
        android:id="@+id/foodBeneficiaryDashboardFragment"
        android:name="com.example.lunchwallet.foodbeneficiary.ui.dashboard.FoodBeneficiaryDashboardFragment"
        android:label="fragment_food_beneficiary_dashboard"
        tools:layout="@layout/fragment_food_beneficiary_dashboard" >
        <action
            android:id="@+id/action_foodBeneficiaryDashboardFragment_to_notificationsScreenFragment2"
            app:destination="@id/notificationsScreenFragment" />
    </fragment>
    <fragment
        android:id="@+id/notificationsScreenFragment"
        android:name="com.example.lunchwallet.onboarding.ui.notifications.ui.NotificationsScreenFragment"
        android:label="fragment_notifications_screen"
        tools:layout="@layout/fragment_notifications_screen" />
    <fragment
        android:id="@+id/kitchenStaffDashBoardFragment"
        android:name="com.example.lunchwallet.kitchenstaff.ui.dashboard.KitchenStaffDashBoardFragment"
